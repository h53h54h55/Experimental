{
  "_manifest" : {
    "name": "Lobstor",
    "id": "lob.express",
    "version": 1.1,
    "classPath": "",
    "permaUrl": "https://raw.githubusercontent.com/h53h54h55/Syncler/experimental/parsetest.json"
  },
    "thepiratebaytext": {
      "name": "TEST",
      "enabled": true,
      "response_type": "text",
      "languages": ["en"],
        "base_url": "https://pirateproxylive.org",
        "fallback_urls": [
          "https://pirateproxy.live",
          "https://thepiratebay.party",
          "https://thepiratebay.org",
          "https://thepiratebay.fun",
          "https://thepiratebay.asia",
          "https://tpb.party",
          "https://thepiratebay3.org",
          "https://thehiddenbay.com",
          "https://piratebay.live",
          "https://thepiratebay.zone"
        ],
        "movie": {
          "query": "/s/?q={query}&page=0&&video=on&orderby=99",
          "keywords": "{title} {year}"
        },
        "season": {
          "query": "/s/?q={query}&page=0&&video=on&orderby=99",
          "keywords": [
            "{title} {seasonCode}",
            "{title} season",
            "{title} {year} {seasonCode}",
            "{title} {year} season"
        ]
        },
        "episode": {
          "query": "/s/?q={query}&page=0&&video=on&orderby=99",
          "keywords": [
            "{title} {episodeCode}",
            "{title} {year} {episodeCode}"
        ]
        },
        "html_parser": {
          "row": "doc.querySelectorAll('td.vertTh')",
          "peers": "row.querySelector('td:nth-child(7)') ? row.querySelector('td:nth-child(7)').textContent : ''",
          "seeds": "row.querySelector('td:nth-child(6)') ? row.querySelector('td:nth-child(6)').textContent : ''",
          "size": "row.querySelector('td:nth-child(5)') ? row.querySelector('td:nth-child(5)').textContent.split(',')[1].replace(/size/i,'') : ''",
          "title": "row.querySelector('div') ? row.querySelector('div').textContent : ''",
          "url": "row.querySelector('td:nth-child(2) a:nth-child(2)') ? row.querySelector('td:nth-child(2) a:nth-child(2)').getAttribute('href') : ''"
        },
        "title_replacement": {
          "\"": "",
          "'s": "s"
        }
      }
}

